.profile
  
  %p
    = image_tag @profile.profile_picture.thumb.url

  %p 
    = @profile.prefered 

  %p 
    = @profile.level 

  %ul 
    - @profile.specialities.each do |speciality|
      %li 
        = speciality.name

= link_to 'edit profile', edit_user_profile_path(@user)
= link_to('edit settings', edit_user_registration_path)
= link_to 'back', user_profile_path(@user)
